{
  "name": "parch",
  "version": "0.0.6",
  "description": "Restify + Sequelize",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d lib",
    "changelog": "conventional-changelog -s -p angular -i CHANGELOG.md",
    "prepare-release": "./scripts/prepare-release",
    "prepublish": "npm test && npm run build",
    "test": "mocha --recursive --compilers js:babel-register",
    "watch": "chokidar 'src/**' 'test/**' -c 'npm test' --initial"
  },
  "keywords": [
    "restify",
    "sequelize",
    "restful"
  ],
  "author": "Dylan Foster",
  "license": "MIT",
  "repository": "dylanfoster/parch",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^2.12.0",
    "eslint-config-unstandard": "^1.3.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "sqlite3": "^3.1.4",
    "supertest": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "unstandard",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "max-len": [1, { "code": 100 }],
      "new-cap": [2, { "properties": false }],
      "no-magic-numbers": [2, { "ignoreArrayIndexes": true }]
    }
  },
  "dependencies": {
    "callsite": "^1.0.0",
    "include-all": "^0.1.6",
    "inflect": "^0.3.0",
    "lodash": "^4.13.1",
    "restify": "^4.1.0",
    "restify-errors": "^4.2.3",
    "sequelize": "^3.23.3"
  }
}
