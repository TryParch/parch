{
  "name": "parch",
  "version": "0.0.1",
  "description": "Restify + Sequelize",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d lib",
    "changelog": "conventional-changelog -s -p angular -i CHANGELOG.md",
    "prepublish": "npm run build",
    "test": "mocha --recursive --compilers js:babel-register",
    "watch": "chokidar 'src/**' 'test/**' -c 'npm test' --initial"
  },
  "keywords": [
    "restify",
    "sequelize",
    "restful"
  ],
  "author": "Dylan Foster",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "conventional-changelog-cli": "^1.2.0",
    "eslint": "^2.12.0",
    "eslint-config-unstandard": "^1.3.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3"
  },
  "eslintConfig": {
    "extends": "unstandard",
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
